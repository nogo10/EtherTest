

contract Math{


	function sum(uint a, uint b) returns (uint result){	    
		result = a + b;
	}
	
	function mul(uint a, uint b) returns (uint result){	    
		result = a * b;
	}
	
	function sub(uint a, uint b) returns (uint result){	    
		result = a - b;
	}
	
	function div(uint a, uint b) returns (uint result){	    
		result = a / b;
	}
	
}